{
  "name": "@aureooms/js-itertools",
  "description": "Iterable and iterator library for JavaScript",
  "version": "4.1.0",
  "license": "AGPL-3.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "homepage": "https://aureooms.github.io/js-itertools",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-itertools"
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-itertools/issues"
  },
  "keywords": [
    "chain",
    "combinations",
    "filter",
    "iter",
    "iterable",
    "iterator",
    "list",
    "map",
    "permutations",
    "product",
    "reduce"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "build-docs": "esdoc",
    "build-gh-pages": "npm run build-docs",
    "commit-msg": "commitlint --edit",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "install-hooks": "husky install",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np",
    "test": "xo && ava"
  },
  "dependencies": {
    "@aureooms/js-collections-deque": "^4.0.1",
    "@aureooms/js-error": "^5.0.0"
  },
  "devDependencies": {
    "@aureooms/js-compare": "1.4.8",
    "@aureooms/js-operator": "1.0.2",
    "@aureooms/js-predicate": "2.0.2",
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/register": "7.13.8",
    "@commitlint/cli": "12.0.1",
    "@js-library/commitlint-config": "0.0.3",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.6.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "husky": "5.1.3",
    "np": "7.4.0",
    "pinst": "2.1.6",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.7",
    "xo": "0.38.2"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "2m",
    "concurrency": 8
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": [
            "defaults",
            "maintained node versions"
          ]
        }
      ]
    ],
    "env": {
      "development": {
        "sourceMaps": "inline",
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ]
      },
      "debug": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": "current node"
            }
          ],
          "babel-preset-power-assert"
        ]
      },
      "production": {}
    }
  },
  "xo": {
    "prettier": true,
    "ignores": [
      "lib"
    ],
    "overrides": [
      {
        "files": "src/base/len.js",
        "rules": {
          "unicorn/prevent-abbreviations": "off"
        }
      },
      {
        "files": "src/base/StopIteration.js",
        "rules": {
          "unicorn/filename-case": [
            "error",
            {
              "case": "pascalCase"
            }
          ]
        }
      },
      {
        "files": [
          "doc/**"
        ],
        "env": "browser"
      }
    ]
  }
}
