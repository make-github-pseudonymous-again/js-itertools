{
  "name": "@aureooms/js-itertools",
  "description": "Iterable and iterator library for JavaScript",
  "version": "3.4.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "sources": [
      "src/**/*"
    ],
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-itertools/issues"
  },
  "dependencies": {
    "@aureooms/js-collections-deque": "^3.1.0",
    "@aureooms/js-error": "^4.0.0"
  },
  "devDependencies": {
    "@aureooms/js-compare": "1.4.5",
    "@aureooms/js-operator": "1.0.2",
    "@aureooms/js-predicate": "2.0.2",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "@babel/register": "7.8.6",
    "ava": "2.4.0",
    "coveralls": "3.0.9",
    "esdoc": "1.1.0",
    "nyc": "14.1.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://aureooms.github.io/js-itertools",
  "keywords": [
    "chain",
    "combinations",
    "filter",
    "iter",
    "iterable",
    "iterator",
    "list",
    "map",
    "permutations",
    "product",
    "reduce"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-itertools"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava --concurrency 8"
  }
}
