{
  "name": "@aureooms/js-itertools",
  "description": "Iterable and iterator library for JavaScript",
  "version": "3.4.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "concurrency": 8,
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-itertools/issues"
  },
  "dependencies": {
    "@aureooms/js-collections-deque": "^3.1.0",
    "@aureooms/js-error": "^4.0.0"
  },
  "devDependencies": {
    "@aureooms/js-compare": "1.4.5",
    "@aureooms/js-operator": "1.0.2",
    "@aureooms/js-predicate": "2.0.2",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "ava": "3.7.1",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "nyc": "15.0.1",
    "xo": "0.30.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://aureooms.github.io/js-itertools",
  "keywords": [
    "chain",
    "combinations",
    "filter",
    "iter",
    "iterable",
    "iterator",
    "list",
    "map",
    "permutations",
    "product",
    "reduce"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-itertools"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "xo && ava"
  },
  "xo": {
    "prettier": true,
    "ignore": [
      "lib",
      "doc"
    ],
    "overrides": [
      {
        "files": "src/base/len.js",
        "rules": {
          "unicorn/prevent-abbreviations": "off"
        }
      },
      {
        "files": "src/base/StopIteration.js",
        "rules": {
          "unicorn/filename-case": [
            "error",
            {
              "case": "pascalCase"
            }
          ]
        }
      }
    ]
  }
}
